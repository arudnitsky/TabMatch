{
    "manifest_version": 2,
  
    "name": "Bookmark All Tabs On Exit",
    "description": "This extension bookmarks all tabs whenever the browser exits.",
    "version": "0.1",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "Bookmark All Tabs On Exit"
    },
    "permissions": [
      "activeTab",
      "storage",
      "tabs",
      "bookmarks",
      "alarms"
    ]
  }