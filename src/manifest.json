{
    "manifest_version": 2,
  
    "name": "TabMatch",
    "description": "Saves all tabs as bookmarks",
    "version": "0.1",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "TabMatch"
    },
    "permissions": [
      "activeTab",
      "storage",
      "tabs",
      "bookmarks",
      "alarms"
    ]
  }